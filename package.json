{
  "name": "shark",
  "version": "1.2.0",
  "private": true,
  "description": "shark is a web application designed to help users manage and visualize shark data. The project is built using the React framework for the frontend interface, utilizing the component library provided by Ant Design to create a user-friendly interface, and managing state using Zustand. Through these technologies, shark provides a powerful and easy-to-use platform for users to effectively manage and display shark data. Additionally, the project uses rspack for building to achieve project bundling and optimization.",
  "author": {
    "name": "h7ml",
    "email": "h7ml@qq.com",
    "url": "https://shark.h7ml.cn",
    "github": "https://github.com/h7ml"
  },
  "license": "Apache-2.0",
  "homepage": "https://shark.h7ml.cn",
  "repository": {
    "type": "git",
    "url": "https://github.com/h7ml/shark.git"
  },
  "bugs": {
    "url": "https://github.com/h7ml/shark/issues"
  },
  "keywords": [
    "shark",
    "web",
    "application",
    "data visualization",
    "React",
    "Ant Design",
    "Zustand"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rspack build",
    "build:analyze": "cross-env NODE_ENV=production rspack build --analyze",
    "build:gh-pages": "cross-env NODE_ENV=production PUBLIC_PATH=/shark/ rspack build",
    "commit": "git-cz",
    "dev": "cross-env NODE_ENV=development rspack serve",
    "edgio:build": "edgio build",
    "edgio:deploy": "edgio deploy",
    "edgio:dev": "edgio dev",
    "eslint": "eslint . --fix --cache",
    "pre-commit": "pretty-quick --staged  && npm run prettier && npm run eslint",
    "prepare": "husky install && npm run pre-commit",
    "prettier": "prettier --write . --log-level error",
    "preview": "npm run build && rspack preview",
    "stylelint": "stylelint \"**/*.{css,scss,sass}\" --fix --cache",
    "release": "release-it",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org h7ml --project shark dist && sentry-cli sourcemaps upload --org h7ml --project shark dist",
    "i18n": "npm run i18n:extract && npm run i18n:replace && npm run i18n:translation",
    "i18n:extract": "node scripts/translation/extract.js",
    "i18n:replace": "node scripts/translation/replace.js",
    "i18n:translation": "node scripts/translation"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.6",
    "@ant-design/plots": "^2.1.15",
    "@ant-design/pro-components": "^2.7.0",
    "@antv/g2plot": "^2.4.31",
    "@rollbar/react": "0.12.0-beta",
    "@sentry/cli": "^2.31.0",
    "@sentry/react": "^7.109.0",
    "@sentry/tracing": "^7.109.0",
    "@sentry/webpack-plugin": "^2.16.1",
    "@types/lodash": "^4.17.0",
    "ahooks": "^3.7.11",
    "antd": "^5.16.1",
    "axios": "^1.6.8",
    "chalk": "^4.1.2",
    "commander": "^12.0.0",
    "fundebug-javascript": "^2.8.8",
    "fundebug-revideo": "^0.8.0",
    "i18next": "^23.11.1",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "mockjs": "^1.1.0",
    "npm-run-all2": "^6.1.2",
    "nprogress": "^0.2.0",
    "ora": "^5.4.1",
    "qs": "^6.12.0",
    "rc-resize-observer": "^1.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "react-use": "^17.5.0",
    "release-it": "^17.1.1",
    "rollbar": "^2.26.4",
    "uuid": "^9.0.1",
    "windicss": "^3.5.6",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.13.2",
    "@antfu/eslint-plugin-prettier": "5.0.1-1",
    "@iconify/json": "^2.2.199",
    "@release-it/conventional-changelog": "^8.0.1",
    "@rspack/cli": "0.6.0",
    "@rspack/core": "0.6.0",
    "@rspack/plugin-minify": "^0.5.9",
    "@rspack/plugin-react-refresh": "0.6.0",
    "@stylistic/stylelint-plugin": "^2.1.1",
    "@types/mockjs": "^1.0.10",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18.2.75",
    "@types/react-dom": "^18.2.24",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "autoprefixer": "^10.4.19",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-git": "^1.9.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-format": "^0.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "postcss-less": "^6.0.0",
    "postcss-loader": "^8.1.1",
    "postcss-windicss": "^1.0.0",
    "prettier": "3.2.5",
    "pretty-quick": "^4.0.0",
    "react-refresh": "^0.14.0",
    "simple-git-hooks": "^2.11.1",
    "stylelint": "^16.3.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^5.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-less": "^3.0.1",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.0",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "node scripts/verify-commit.js"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --cache"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ]
}
